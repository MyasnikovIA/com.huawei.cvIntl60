<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/dialogButtonPressBg"/>
            <corners android:bottomRightRadius="5dp"/>
            <stroke
                android:width="1dp"
                android:color="@color/dialogButtonStroke"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/dialogButtonNormalBg"/>
            <corners android:bottomRightRadius="5dp"/>
            <stroke
                android:width="1dp"
                android:color="@color/dialogButtonStroke"/>
        </shape>
    </item>
</selector>
