            android:id="@+id/LL_ui_singleview_TopLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="1600">
            <LinearLayout
                android:orientation="vertical"
                android:visibility="visible"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:weightSum="900"
                android:layout_weight="1495">
                <FrameLayout
                    android:visibility="visible"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="112">
                    <LinearLayout
                        android:id="@+id/LL_ui_singleview_NormalTopLayout"
                        android:background="@drawable/gallery_toolbar"
                        android:visibility="visible"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:weightSum="1495">
                        <ImageButton
                            android:id="@+id/IB_ui_singleview_btnReturn"
                            android:background="@color/transparent"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/setting_tabarrow"
                            android:scaleType="fitCenter"
                            android:layout_weight="123"
                            style="android:imageButtonStyle"/>
                        <TextView
                            android:ellipsize="end"
                            android:id="@+id/TV_ui_singleview_title"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="472"/>
                        <ImageButton
                            android:textColor="@color/white"
                            android:gravity="center"
                            android:id="@+id/IB_ui_singleview_btnDisplayModeTab"
                            android:background="@color/transparent"
                            android:visibility="invisible"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:scaleType="fitCenter"
                            android:layout_weight="140"/>
                        <ImageView
                            android:id="@+id/IV_ui_singleview_divider"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:src="@drawable/gallery_button_divider"
                            android:scaleType="centerCrop"
                            android:layout_weight="80"/>
                        <FrameLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="140">
                            <ImageButton
                                android:textColor="@color/white"
                                android:gravity="center"
                                android:id="@+id/IB_ui_singleview_btnCreateMovieTab"
                                android:background="@color/transparent"
                                android:visibility="invisible"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:scaleType="fitCenter"/>
                            <ImageButton
                                android:textColor="@color/white"
                                android:gravity="center"
                                android:id="@+id/IB_ui_singleview_btnCreate360StitchMovieTab"
                                android:background="@color/transparent"
                                android:visibility="invisible"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:scaleType="fitCenter"/>
                        </FrameLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="20"/>
                        <ImageButton
                            android:textColor="@color/white"
                            android:gravity="center"
                            android:id="@+id/IB_ui_singleview_btnStreamingVideoTab"
                            android:background="@color/transparent"
                            android:visibility="invisible"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:scaleType="fitCenter"
                            android:layout_weight="140"/>
                        <ImageButton
                            android:textColor="@color/white"
                            android:gravity="center"
                            android:id="@+id/IB_ui_singleview_btnShareTab"
                            android:background="@color/transparent"
                            android:visibility="invisible"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:scaleType="fitCenter"
                            android:layout_weight="140"/>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="20"/>
                        <ImageButton
                            android:textColor="@color/white"
                            android:gravity="center"
                            android:id="@+id/IB_ui_singleview_btnDeleteTab"
                            android:background="@color/transparent"
                            android:visibility="invisible"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:scaleType="fitCenter"
                            android:layout_weight="140"/>
                    </LinearLayout>
                </FrameLayout>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/LL_ui_singleview_NavigationBar"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="105"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/LL_ui_singleview_360ButtonLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="1600">
            <FrameLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:weightSum="900"
                android:layout_weight="1495">
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:id="@+id/LL_ui_singleview_Previous"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageButton
                        android:id="@+id/IB_ui_singleview_Previous"
                        android:background="@color/transparent"
                        android:visibility="invisible"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:scaleType="fitCenter"/>
                </LinearLayout>
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:id="@+id/LL_ui_singleview_Next"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageButton
                        android:id="@+id/IB_ui_singleview_Next"
                        android:background="@color/transparent"
                        android:visibility="invisible"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:scaleType="fitCenter"/>
                </LinearLayout>
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageButton
                        android:id="@+id/IB_ui_singleview_FileType"
                        android:background="@color/transparent"
                        android:visibility="invisible"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:scaleType="fitCenter"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ImageButton
                        android:id="@+id/IB_ui_singleview_StopPlayVideo"
                        android:background="@color/transparent"
                        android:visibility="invisible"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:scaleType="fitCenter"/>
