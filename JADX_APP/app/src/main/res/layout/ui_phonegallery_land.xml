                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="112"/>
                    <LinearLayout
                        android:orientation="horizontal"
                        android:id="@+id/LL_ui_multiview_MenuBackgroundLayout"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="206">
                        <FrameLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:weightSum="1495">
                                <LinearLayout
                                    android:orientation="vertical"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="45"/>
                                <LinearLayout
                                    android:orientation="vertical"
                                    android:id="@+id/LL_ui_multiview_SwitchMenuBackground"
                                    android:background="@drawable/gallery_gallerylist"
                                    android:visibility="invisible"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="608">
                                    <ListView
                                        android:id="@+id/LV_ui_multiview_switchmenu_listOption"
                                        android:scrollbars="vertical"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"/>
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:weightSum="1495">
                                <LinearLayout
                                    android:orientation="vertical"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="833"/>
                                <LinearLayout
                                    android:orientation="vertical"
                                    android:id="@+id/LL_ui_multiview_SortMenuBackground"
                                    android:background="@drawable/gallery_gallerylist"
                                    android:visibility="invisible"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="608">
                                    <ListView
                                        android:id="@+id/LV_ui_multiview_sortmenu_listOption"
                                        android:scrollbars="vertical"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"/>
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:weightSum="1495">
                                <LinearLayout
                                    android:orientation="vertical"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="833"/>
                                <LinearLayout
                                    android:orientation="vertical"
                                    android:id="@+id/LL_ui_multiview_SelectAllMenuBackground"
                                    android:background="@drawable/gallery_gallerylist"
                                    android:visibility="invisible"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="608">
                                    <ListView
                                        android:id="@+id/LV_ui_multiview_selectallmenu_listOption"
                                        android:scrollbars="vertical"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"/>
                                </LinearLayout>
                            </LinearLayout>
                        </FrameLayout>
                    </LinearLayout>
                </LinearLayout>
                <FrameLayout
                    android:id="@+id/FL_ui_multiview_BusyLayout"
                    android:background="@color/blackt50"
                    android:visibility="invisible"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">
                    <ProgressBar
                        android:layout_gravity="center"
                        android:id="@+id/progressBar1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        style="?android:attr/progressBarStyleLarge"/>
                </FrameLayout>
            </FrameLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/LL_ui_multiview_NavigationBar"
                android:background="@color/black"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="105"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/LL_ui_Connect_MainLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="1600">
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/LL_ui_Connect_ViewContent"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1495"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:id="@+id/LL_ui_ModeChange_MainLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:weightSum="1600">
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/LL_ui_ModeChange_ViewContent"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1495"/>
        </LinearLayout>
    </FrameLayout>
    <FrameLayout
        android:id="@+id/FL_ui_phonegallery_SingleLayout"
        android:visibility="invisible"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <FrameLayout
            android:id="@+id/LL_ui_singleview_SingleViewLayout"
            android:background="@color/black"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ui_Controller.CustomWidget.SingleHorizontalScrollView.SingleHorizontalScrollView
                android:id="@+id/LHSV_ui_singleview_ImageView"
                android:scrollbars="none"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_gravity="center_vertical"
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            </ui_Controller.CustomWidget.SingleHorizontalScrollView.SingleHorizontalScrollView>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/LL_ui_singleview_VideoPlayer"
                android:background="@color/black"
                android:visibility="invisible"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <GeneralFunction.Player.player.SphericalVideoPlayer
                    android:id="@+id/TV_ui_singleview_VideoPlayer"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
            </LinearLayout>
            <LinearLayout
                android:layout_gravity="bottom"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:weightSum="900">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="795"/>
                <LinearLayout
                    android:id="@+id/LL_ui_singleview_ListViewLayout"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:weightSum="1600"
                    android:layout_weight="105">
                    <FrameLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1495">
                        <ui_Controller.CustomWidget.ListHorizontalScrollView.ListHorizontalScrollView
                            android:id="@+id/LHSV_ui_singleview_ListView"
                            android:scrollbars="none"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">
                            <LinearLayout
                                android:layout_gravity="center_vertical"
                                android:orientation="horizontal"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>
                        </ui_Controller.CustomWidget.ListHorizontalScrollView.ListHorizontalScrollView>
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
        <LinearLayout
