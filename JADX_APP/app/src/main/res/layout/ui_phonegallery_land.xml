                                android:id="@+id/B_ui_multiview_Delete"
                                android:background="@color/gray"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:text="@string/delete"
                                android:layout_weight="747"/>
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>
